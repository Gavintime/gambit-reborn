{
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "ignorePatterns": ["dist"]
}
